{
    "manifest_version" : 2,
    "name" : "Whatsapp Web Speed Audio",
    "version" : "1.0.0",
    "description" : "Acelere ou desacelere os audios do Whatsapp Web.",
    "author" : "Rafael WMS",

    "icons": {
        "128" : "128.png"
    },

    "browser_action": {
   	 "default_icon": "128.png"
    },

    "content_scripts" : [
        {
            "matches" : ["https://web.whatsapp.com/"],
            "css" : ["style.css"],
            "js" : ["content.js"]
        }
    ]
}